{
  "name": "@hatchifyjs/react-ui",
  "version": "0.1.21",
  "main": "./dist/react-ui.js",
  "module": "./dist/react-ui.mjs",
  "types": "./dist/react-ui.d.ts",
  "exports": {
    ".": {
      "require": "./dist/react-ui.js",
      "import": "./dist/react-ui.mjs",
      "types": "./src/react-ui.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "eslint": "eslint vite.config.ts src",
    "depcheck": "depcheck .",
    "dev": "vite",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf tsconfig.tsbuildinfo dist",
    "build": "vite build"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.32.2",
    "vite": "^4.3.1",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "@hatchifyjs/react-rest": "^0.1.11",
    "@hatchifyjs/rest-client": "^0.1.9"
  },
  "peerDependencies": {
    "@hatchifyjs/core": "^0.3.x",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0"
  }
}

{
  "name": "@hatchifyjs/rest-client-jsonapi",
  "version": "0.1.18",
  "main": "./dist/rest-client-jsonapi.js",
  "module": "./dist/rest-client-jsonapi.mjs",
  "types": "./dist/rest-client-jsonapi.d.ts",
  "exports": {
    ".": {
      "require": "./dist/rest-client-jsonapi.js",
      "import": "./dist/rest-client-jsonapi.mjs",
      "types": "./dist/rest-client-jsonapi.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint vite.config.ts src",
    "depcheck": "depcheck .",
    "dev": "vite",
    "test": "TZ=UTC vitest run",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf tsconfig.tsbuildinfo dist",
    "build": "vite build"
  },
  "dependencies": {
    "@hatchifyjs/rest-client": "0.1.14"
  },
  "devDependencies": {
    "@hatchifyjs/koa": "^1.0.2",
    "@hatchifyjs/react-rest": "^0.1.18",
    "@vitest/coverage-c8": "^0.32.2",
    "koa": "^2.14.2",
    "msw": "^1.2.1",
    "vite": "^4.3.1",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.30.1"
  },
  "peerDependencies": {
    "@hatchifyjs/core": "^0.3.x"
  }
}

{
  "name": "@hatchifyjs/react-jsonapi",
  "version": "0.0.1-0",
  "main": "./dist/react-jsonapi.js",
  "module": "./dist/react-jsonapi.mjs",
  "types": "./dist/react-jsonapi.d.ts",
  "exports": {
    ".": {
      "require": "./dist/react-jsonapi.js",
      "import": "./dist/react-jsonapi.mjs",
      "types": "./src/react-jsonapi.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "eslint": "eslint vite.config.ts src",
    "depcheck": "depcheck .",
    "dev": "vite",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf tsconfig.tsbuildinfo dist",
    "build": "vite build"
  },
  "dependencies": {
    "@hatchifyjs/react-rest": "^0.0.1-1",
    "@hatchifyjs/rest-client-jsonapi": "^0.0.1-2"
  },
  "keywords": [
    "react",
    "jsonapi",
    "react-jsonapi"
  ]
}

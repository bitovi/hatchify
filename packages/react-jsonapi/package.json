{
  "name": "@hatchifyjs/react-jsonapi",
  "version": "0.1.1",
  "main": "./dist/react-jsonapi.js",
  "module": "./dist/react-jsonapi.mjs",
  "types": "./dist/react-jsonapi.d.ts",
  "exports": {
    ".": {
      "require": "./dist/react-jsonapi.js",
      "import": "./dist/react-jsonapi.mjs",
      "types": "./src/react-jsonapi.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "eslint": "eslint vite.config.ts src",
    "depcheck": "depcheck .",
    "dev": "vite",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf tsconfig.tsbuildinfo dist",
    "build": "vite build"
  },
  "dependencies": {
    "@hatchifyjs/core": "^0.2.5",
    "@hatchifyjs/react-rest": "^0.1.2",
    "@hatchifyjs/rest-client": "^0.1.2",
    "@hatchifyjs/rest-client-jsonapi": "^0.1.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.32.2",
    "vite": "^4.3.1",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.30.1"
  },
  "keywords": [
    "react",
    "jsonapi",
    "react-jsonapi"
  ]
}
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2"
  },
  "peerDependencies": {
    "@hatchifyjs/core": "^0.3.x",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"

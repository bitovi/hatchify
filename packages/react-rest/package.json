{
  "name": "@hatchifyjs/react-rest",
  "private": true,
  "version": "0.0.0",
  "main": "./dist/react-rest.js",
  "module": "./dist/react-rest.mjs",
  "types": "./dist/react-rest.d.ts",
  "exports": {
    ".": {
      "require": "./dist/react-rest.js",
      "import": "./dist/react-rest.mjs",
      "types": "./src/react-rest.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "eslint": "eslint vite.config.ts src",
    "depcheck": "depcheck .",
    "dev": "vite",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf tsconfig.tsbuildinfo dist",
    "build": "vite build"
  },
  "dependencies": {
    "@hatchifyjs/rest-client": "^0.0.0",
    "@hatchifyjs/hatchify-core": "^0.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.0",
    "jsdom": "^21.1.1",
    "vite": "^4.3.1",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.30.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}

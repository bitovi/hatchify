{
  "name": "@hatchifyjs/rest-client",
  "version": "0.0.1-4",
  "main": "./dist/rest-client.js",
  "module": "./dist/rest-client.mjs",
  "types": "./dist/rest-client.d.ts",
  "exports": {
    ".": {
      "require": "./dist/rest-client.js",

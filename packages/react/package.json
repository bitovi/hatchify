{
  "name": "@hatchifyjs/react",
  "version": "0.0.1-29",
  "main": "./dist/react.js",
  "module": "./dist/react.mjs",
  "types": "./dist/react.d.ts",
  "exports": {
    ".": {
      "require": "./dist/react.js",
      "import": "./dist/react.mjs",
      "types": "./src/react.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "eslint": "eslint vite.config.ts src",
    "depcheck": "depcheck .",
    "dev": "vite",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf tsconfig.tsbuildinfo dist",
    "build": "vite build"
  },
  "dependencies": {
    "@hatchifyjs/design-mui": "^0.0.1-23",
    "@hatchifyjs/hatchify-core": "^0.0.1-1",
    "@hatchifyjs/react-ui": "^0.0.1-17",
    "@hatchifyjs/rest-client-jsonapi": "^0.0.1-17"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.32.2",
    "jsdom": "^21.1.1",
    "vite": "^4.3.1",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.30.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}

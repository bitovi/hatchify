  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^4.3.1",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hatchifyjs/react-ui": "^0.0.1-6",
    "@mui/material": "^5.12.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }

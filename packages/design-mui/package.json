{
  "name": "@hatchifyjs/design-mui",
  "version": "0.1.37",
  "main": "./dist/design-mui.js",
  "module": "./dist/design-mui.mjs",
  "types": "./dist/design-mui.d.ts",
  "exports": {
    ".": {
      "require": "./dist/design-mui.js",
      "import": "./dist/design-mui.mjs",
      "types": "./src/design-mui.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint vite.config.ts src",
    "depcheck": "depcheck .",
    "dev": "vite",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf tsconfig.tsbuildinfo dist",
    "build": "vite build"
  },
  "devDependencies": {
    "@hatchifyjs/rest-client": "^0.1.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/react": "^18.0.28",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.32.2",
    "vite": "^4.3.1",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "@hatchifyjs/react-ui": "^0.1.27",
    "@mui/icons-material": "^5.14.1",
    "@mui/utils": "^5.13.1",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "@hatchifyjs/core": "^0.3.x",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}

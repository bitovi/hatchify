{
  "name": "hatchify-core",
  "private": true,
  "version": "0.0.0",
  "main": "./dist/hatchify-core.js",
  "module": "./dist/hatchify-core.mjs",
  "types": "./dist/hatchify-core.d.ts",
  "exports": {
    ".": {
      "require": "./dist/hatchify-core.js",
      "import": "./dist/hatchify-core.mjs",
      "types": "./src/hatchify-core.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "eslint": "eslint vite.config.ts src",
    "prettier": "prettier --check vite.config.ts src",
    "depcheck": "depcheck .",
    "dev": "vite",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf tsconfig.tsbuildinfo dist",
    "build": "vite build"
  },
  "devDependencies": {
    "vite": "^4.3.1",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.30.1"
  }
}
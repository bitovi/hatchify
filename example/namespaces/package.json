{
  "name": "namespaces",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc --outDir dist/backend --project tsconfig.backend.json",
    "build:frontend": "vite build --outDir dist/frontend",
    "dev": "nodemon backend/index.ts --watch backend --watch schemas.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start:backend": "node backend/index.js"
  },
  "dependencies": {
    "@hatchifyjs/core": "^0.3.22",
    "@hatchifyjs/express": "^1.3.49",
    "@hatchifyjs/react-rest": "^0.1.28",
    "express": "^4.19.2",
    "lodash.debounce": "^4.0.8",
    "sequelize": "^6.37.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hatchifyjs/react": "^0.1.69",
    "@mui/material": "^5.15.15",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "nodemon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.8"
  }
}

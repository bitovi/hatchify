{
  "name": "hatchify-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "dev": "nodemon backend/index.ts --watch backend --watch schemas.ts",
    "build:frontend": "tsc && vite build --outDir dist/frontend",
    "build:backend": "tsc --outDir dist/backend --project tsconfig.backend.json",
    "start:frontend": "vite preview --outDir dist/frontend",
    "start:backend": "node dist/backend/backend/index.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hatchifyjs/core": "file:../../packages/core",
    "@hatchifyjs/koa": "file:../../packages/koa",
    "@hatchifyjs/react": "file:../../packages/react",
    "@mui/material": "^5.15.2",
    "koa": "^2.14.2",
    "koa-connect": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/koa": "^2.13.12",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.10"
  }
}

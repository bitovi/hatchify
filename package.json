{
  "name": "nx-package-based",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "check-packages": "ts-node ./scripts/check-packages"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@bitovi/eslint-config/react",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "curly": "error",
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "trailingComma": "all"
        }
      ]
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@bitovi/eslint-config": "^1.6.0",
    "@playwright/test": "^1.35.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nx": "15.9.2",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "semver": "^7.5.0"
  }
}

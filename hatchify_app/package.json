{
  "name": "hatchify_app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "dev": "nodemon --esm backend/index.ts --watch backend --watch schemas.ts",
    "build:frontend": "tsc && vite build --outDir dist/frontend",
    "build:backend": "tsc --outDir dist/backend --project tsconfig.backend.json",
    "start:frontend": "vite preview --outDir dist/frontend",
    "start:backend": "node dist/backend/backend/index.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hatchifyjs/core": "^0.3.4",
    "@hatchifyjs/koa": "^1.1.0",
    "@hatchifyjs/react": "^0.1.21",
    "@mui/material": "^5.14.18",
    "koa": "^2.14.2",
    "koa-connect": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.35.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/koa": "^2.13.12",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.2"
  }
}
